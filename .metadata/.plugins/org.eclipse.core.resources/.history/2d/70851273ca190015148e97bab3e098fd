var launchWin = Ti.UI.createWindow({
	backgroundColor: "#fffff0",
	title: "Launch"
});

var launchView = Ti.UI.createView({
	top: 20,
});

var launchTable = Ti.UI.createTableView({
	//Set styling of table view to grouped
	style: Ti.UI.iPhone.TableViewStyle.GROUPED
});

var launchSection = Ti.UI.createTableViewSection({
	headerTitle: "Launch Sections"
});

var calcRow = Ti.UI.createTableViewRow({
	title: "Merge Calculator",
	hasChild: true
});

var aboutRow = Ti.UI.createTableViewRow({
	title: "About this program",
	hasChild: true
});

launchSection.add(calcRow, aboutRow);
launchTable.add(launchSection);
launchView.add(launchTable);
launchWin.add(launchView);

exports.launchCall = launchWin;